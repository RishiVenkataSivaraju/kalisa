    <script>
        document.querySelectorAll('.room-img').forEach(img => {
            img.addEventListener('click', () => {
                window.location.href = "/rooms"; // Instantly hits app.get("/rooms")
            });
        });
        document.addEventListener("DOMContentLoaded", function () {
            const square = document.querySelector(".square");

            for (let i = 0; i < 20; i++) {
                let line = document.createElement("div");
                line.classList.add("line");
                line.style.left = `${i * 15}px`;
                line.style.animationDelay = `${i * 0.1}s`;
                square.appendChild(line);
            }
        });
        document.addEventListener("DOMContentLoaded", function () {
            const amenities = document.querySelectorAll(".amenity");
            const observerOptions = {
                threshold: 0.2
            };

            const observer = new IntersectionObserver((entries) => {
                entries.forEach((entry, index) => {
                    if (entry.isIntersecting) {
                        setTimeout(() => {
                            entry.target.classList.add("show");
                        }, index * 100);
                    }
                });
            }, observerOptions);

            amenities.forEach((amenity) => {
                observer.observe(amenity);
            });
        });
        document.getElementById("scrollButton").addEventListener("click", function () {
            document.getElementById("bottom").scrollIntoView({ behavior: "smooth" });
        });
    </script>
